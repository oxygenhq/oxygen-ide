{
  "name": "oxygenide",
  "productName": "Oxygen IDE",
  "version": "1.15.0",
  "description": "IDE for Oxygen automation framework.",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors",
    "build-main": "cross-env NODE_ENV=production node --max-old-space-size=3072 --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --max-old-space-size=3072 --trace-warnings -r @babel/register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "dev": "cross-env START_HOT=1 node -r @babel/register ./internals/scripts/CheckPortInUse.js && cross-env START_HOT=1 npm run start-renderer-dev",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed update --ignoreDeps dev || (exit 0)",
    "lint": "cross-env NODE_ENV=development eslint '**/*.{js,jsx}' --quiet",
    "package": "npm install && cross-env RELEASE_BUILD=true npm run build && grunt",
    "postinstall": "node -r @babel/register internals/scripts/CheckNativeDep.js && npm run flow-typed && npm run build-dll && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && cd app && npm install",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production electron -r @babel/register ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron ./wrap-main.dev.js",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r @babel/register  ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r @babel/register ./internals/scripts/RunTests.js",
    "test1": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r @babel/register ./node_modules/.bin/jest test/main/services/",
    "test2": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r @babel/register ./app/main/test.js",
    "test-renderer-store": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r @babel/register ./node_modules/.bin/jest test/unit/renderer/store/",
    "test-file-watcher": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r @babel/register ./node_modules/jest/bin/jest test/file-watcher/",
    "test-all": "npm run lint && npm run flow && npm run build && npm run test && npm run test-e2e",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r @babel/register ./internals/scripts/RunTests.js e2e",
    "test-watch": "npm test -- --watch"
  },
  "browserslist": "electron 4.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxygenhq/oxygen-ide.git"
  },
  "author": {
    "name": "cloudbeat.io",
    "email": "ndimer@cloudbeat.io"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/oxygenhq/oxygen-ide/issues"
  },
  "homepage": "https://github.com/oxygenhq/oxygen-ide",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "electron": "<rootDir>/test/__mocks__/electron.js"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(antd)/)"
    ],
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ],
    "testURL": "file:/",
    "verbose": true,
    "useStderr": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "devDependencies": {
