{
  "name": "oxygenide",
  "productName": "Oxygen IDE",
  "version": "1.6.0",
  "description": "IDE for Oxygen automation framework.",
  "scripts": {
    "build": "concurrently \"npm run build-main\" \"npm run build-renderer\"",
    "build-dll": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.dev.dll.js --colors",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.main.prod.js --colors",
    "build-renderer": "cross-env NODE_ENV=production node --trace-warnings -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.renderer.prod.js --colors",
    "dev": "cross-env START_HOT=1 node -r babel-register ./internals/scripts/CheckPortInUse.js && cross-env process.env._TESTING=1 START_HOT=1 npm run start-renderer-dev",
    "flow": "flow",
    "flow-typed": "rimraf flow-typed/npm && flow-typed install --ignoreDeps dev --overwrite || (exit 0)",
    "lint": "cross-env NODE_ENV=development eslint --cache --format=node_modules/eslint-formatter-pretty .",
    "lint-fix": "npm run lint -- --fix",
    "lint-styles": "stylelint app/*.css app/components/*.css --syntax scss",
    "lint-styles-fix": "stylefmt -r app/*.css app/components/*.css",
    "package": "npm install && npm run build && grunt",
    "postinstall": "node -r babel-register internals/scripts/CheckNativeDep.js && npm run flow-typed && npm run build-dll && node node_modules/fbjs-scripts/node/check-dev-engines.js package.json && cd app && npm install",
    "prestart": "npm run build",
    "start": "cross-env process.env._TESTING=1 NODE_ENV=production electron ./app/",
    "start-main-dev": "cross-env HOT=1 NODE_ENV=development electron --inspect=5858 -r babel-register ./app/main/main.dev.js",
    "start-renderer-dev": "cross-env NODE_ENV=development node --trace-warnings -r babel-register ./node_modules/webpack-dev-server/bin/webpack-dev-server --config webpack.config.renderer.dev.js",
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r babel-register ./internals/scripts/RunTests.js",
    "test1": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r babel-register ./node_modules/.bin/jest test/main/services/",
    "test2": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r babel-register ./app/main/test.js",
    "test-renderer-store": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r babel-register ./node_modules/.bin/jest test/unit/renderer/store/",
    "test-file-watcher": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r babel-register ./node_modules/jest/bin/jest test/file-watcher/",
    "test-all": "npm run lint && npm run flow && npm run build && npm run test && npm run test-e2e",
    "test-e2e": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 node --trace-warnings -r babel-register ./internals/scripts/RunTests.js e2e",
    "test-watch": "npm test -- --watch"
  },
  "browserslist": "electron 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oxygenhq/oxygen-ide.git"
  },
  "author": {
    "name": "cloudbeat.io",
    "email": "ndimer@cloudbeat.io"
  },
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/oxygenhq/oxygen-ide/issues"
  },
  "homepage": "https://github.com/oxygenhq/oxygen-ide",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy",
      "electron": "<rootDir>/test/__mocks__/electron.js"
    },
    "moduleFileExtensions": [
      "js"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/node_modules"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(antd)/)"
    ],
    "setupFiles": [
      "./internals/scripts/CheckBuiltsExist.js"
    ],
    "testURL": "file:/",
    "verbose": true,
    "useStderr": true
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-flow-runtime": "^0.17.0",
    "babel-plugin-import": "^1.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chalk": "^2.3.1",
    "concurrently": "^3.5.1",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.1.6",
    "cross-spawn": "^6.0.4",
    "css-loader": "^0.28.10",
    "electron": "^3.1.8",
    "electron-devtools-installer": "^2.2.4",
    "electron-rebuild": "^1.8.4",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-formatter-pretty": "^1.3.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.12.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fbjs-scripts": "^1.2.0",
    "file-loader": "^1.1.10",
    "flow-bin": "^0.80.0",
    "flow-runtime": "^0.17.0",
    "flow-typed": "^2.5.1",
    "grunt": "^1.0.3",
    "grunt-chmod": "^1.1.1",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-compress": "^1.4.3",
    "grunt-contrib-copy": "^1.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.1.4",
    "mini-css-extract-plugin": "^0.4.3",
    "modclean": "^2.1.2",
    "node-abi": "^2.8.0",
    "node-sass": "^4.7.2",
    "postcss-discard-font-face": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "recursive-copy": "^2.0.9",
    "redux-logger": "^3.0.6",
    "repeat-string": "^1.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^0.6.2",
    "webpack": "^4.19.1",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.8",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "antd": "^3.4.5",
    "appium-adb": "^7.6.5",
    "clean-stack": "^2.1.0",
    "detect-port": "^1.3.0",
    "devtron": "^1.4.0",
    "electron-debug": "^2.2.0",
    "electron-settings": "^3.2.0",
    "firacode": "^1.206.0",
    "font-awesome": "^4.7.0",
    "fs-extra": "^7.0.1",
    "junk": "^3.1.0",
