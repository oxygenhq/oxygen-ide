    "mixpanel": "^0.10.2",
    "moment": "^2.24.0",
    "os-locale": "^3.1.0",
    "perfect-scrollbar": "^1.4.0",
    "react": "^16.3.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.3.2",
    "react-hot-loader": "^4.8.3",
    "react-icons": "^2.2.7",
    "react-redux": "^5.1.1",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.3",
